    <script>
        $(document).ready(function () {
            var teamid = [];
            var equal;


            for (var i = 0; i < $("div").filter(".team-details")
                .length; i++) { //creates an array of id values of all the team divs.
                teamid.push($(".team-details").eq(i).attr("id"));
            }

            //Show only current team while loading for first time or refreshing the page
            var firstTimeLoad = 1;
            while (firstTimeLoad) {
                $('div').filter(".team-details").not(":eq(0)").hide();
                firstTimeLoad = 0;
            }


            $('ul.nav li a').on('click', function () {

                if ($(this).hasClass("active")) {
                    return false;
                } else {
                    $("a").filter('.nav-link').removeClass('active');
                    $(this).addClass('active');

                    var navid = $("a").filter(".active").attr(
                        "data-link"
                        ); //sets the id text of nav element with active class to the var navid

                    for (var j = 0; j < teamid.length; j++) {
                        if (teamid[j] == navid) {

                            $("div").filter(".team-details").hide(300);
                            $("div").filter("#" + navid).show(300);
                            break;
                        }

                    }

                }

            });

















        });
    </script>


